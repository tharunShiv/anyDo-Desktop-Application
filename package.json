{
  "name": "anydo-desktop-application",
  "version": "1.0.0",
  "description": "todolist",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icon/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icon/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"anyDo todo\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icon/linux/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tharunShiv/anyDo-Desktop-Application.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tharunShiv/anyDo-Desktop-Application/issues"
  },
  "homepage": "https://github.com/tharunShiv/anyDo-Desktop-Application#readme",
  "dependencies": {
    "electron": "^15.5.5"
  },
  "devDependencies": {
    "electron-packager": "^13.1.1"
  }
}
